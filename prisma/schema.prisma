datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  name      String   // Add this line if missing
  email     String   @unique
  password  String
  role      String   @default("user")
  createdAt DateTime @default(now())
}
